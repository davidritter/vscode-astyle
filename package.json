{
    "name": "vscode-astyle",
    "displayName": "Astyle",
    "description": "Format C/C++/Obj-C/C#/Java code with Astyle in VSCode",
    "version": "0.7.1",
    "publisher": "chiehyu",
    "repository": {
        "type": "git",
        "url": "https://github.com/welkineins/vscode-astyle"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other",
        "Languages"
    ],
    "keywords": [
        "C",
        "C++",
        "Objective-C",
        "C#",
        "Java"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Astyle configuration",
            "properties": {
                "astyle.executable": {
                    "type": "string",
                    "default": "astyle",
                    "description": "astyle executable path"
                },
                "astyle.astylerc": {
                    "type": ["string", "null"],
                    "default": null,
                    "description": "astylerc path (set to null for default lookup order)"
                },
                "astyle.cmd_options": {
                    "type": ["array"],
                    "default": [],
                    "description": "array of command line options"
                },
                "astyle.max_buffer_multipler": {
                    "type": "string",
                    "default": "2",
                    "description": "the multiplier for the new size of a file after formatting"
                },
                 "astyle.additional_languages": {
                    "type": ["array"],
                    "default": [],
                    "description": "array of additional languages to use astyle as formatter"
                },
                "astyle.c.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "enable C formatting"
                },
                "astyle.cpp.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "enable C++ formatting"
                },
                "astyle.objective-c.enable": {
                    "type": "boolean",
                    "default": false,
                    "description": "enable Object-C formatting"
                },
                "astyle.csharp.enable": {
                    "type": "boolean",
                    "default": false,
                    "description": "enable C# formatting"
                },
                "astyle.java.enable": {
                    "type": "boolean",
                    "default": false,
                    "description": "enable Java formatting"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "diff-match-patch": "^1.0.0"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    }
}